pipeline{
      
      agent any
      stages{
        stage('Hello_world'){
            steps
            {
                echo 'hello I am Vinay'
                echo 'welcome to calsoft'
            }
        }
        stage('build'){
            steps
            {
                echo 'build done'
            }
        }
        stage('deploy'){
            steps
            {
                echo 'deploy done'
            }
        }
        stage('test'){
            steps
            {
                echo 'testing done'
            }
        }
        
        stage('intern'){
              steps
              {
                  echo 'I m an intern at Calsoft'  
              }
              
        }
        
            stage('holiday'){
                  steps
                  {
                        echo 'Holiday start'
                  }
            }
                  
    }
}
